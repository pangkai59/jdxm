<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jdxm.mapper.base.SysLogMapper" >


  <insert id="insert"   parameterType="com.jdxm.entity.basic.SysLog"  >
      insert into sys_log
      (user_id,params,method,url,ip,update_time,create_time )
      VALUE
      (#{userId},#{params},#{method},#{url},#{ip},#{updateTime},#{createTime}  )
  </insert>



</mapper>